#include<iostream>
using namespace std;

int num1,num2;

int plus(int x, int y)
{
	return(x+y);
}
int minus(int x, int y)
{
	return(x-y);
}
int divide(int x, int y)
{
	return(x/y);
}
int multi(int x,int y)
{
	return(x*y);
}




int main(void)
{
	cout<<"input num1 : ";
	cin>>num1;
	cout<<"input num2 : ";
	cin>>num2;

	cout<<"result num1+num2 = "<<plus(num1,num2)<<endl;

	cout<<"result num1-num2 = "<<minus(num1,num2)<<endl;

	cout<<"result num1*num2 = "<<multi(num1,num2)<<endl;

	cout<<"result num1/num2 = "<<divide(num1,num2)<<endl;

}
